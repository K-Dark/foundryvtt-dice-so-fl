{
	"name": "dice-so-fl",
	"title": "Dice So Forbidden Lands!",
	"description": "A Forbidden Lands integration of the Dice so Nice! module.",
	"version": "0.0.1",
	"minimumCoreVersion" : "0.6.6",
	"compatibleCoreVersion" : "0.7.0",
	"author": "Keran",
	"esmodules": [
		"./main.js"
	],
	"socket": true,
	"styles": [
		"./style.css"
	],
	"languages": [{
		"lang": "en",
		"name": "English",
		"path": "./lang/en.json"
	}, {
		"lang": "fr",
		"name": "French",
		"path": "./lang/fr.json"
	}],
    "dependencies": [
        {
          "name": "forbidden-lands",
          "type": "system",
          "manifest": "https://raw.githubusercontent.com/Perfectro/forbidden-lands-foundry-vtt/master/system.json"
        },
        {
          "name": "dice-so-nice",
          "manifest": "https://gitlab.com/riccisi/foundryvtt-dice-so-nice/raw/master/module/module.json",
          "type": "module"
        }
    ],
	"url": "https://github.com/K-Dark/foundryvtt-dice-so-fl",
	"manifest": "https://raw.githubusercontent.com/K-Dark/foundryvtt-dice-so-fl/main/module.json",
	"__download": "https://gitlab.com/riccisi/foundryvtt-dice-so-nice/-/jobs/artifacts/2.0.4/raw/dice-so-nice.zip?job=build"
}